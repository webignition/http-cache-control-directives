{
    "name": "webignition/http-cache-control-directives",
    "description": "Model of a web-based resource",
    "keywords": ["http", "header", "cache-control"],
    "homepage": "https://github.com/webignition/http-cache-control-directives",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\HttpCacheControlDirectives\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\HttpCacheControlDirectives\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3"
    },
    "minimum-stability":"stable",
    "prefer-stable":true
}